{
  "schemaVersion": 1,
  "id": "customizableelytra",
  "version": "${version}",
  "name": "Customizable Elytra",
  "description": "Allows elytra to be dyed like leather armor and have banners placed on them like shields",
  "authors": [
    "Hidoni"
  ],
  "contact": {
    "repo": "https://github.com/Hidoni/CustomizableElytra"
  },
  "license": "MIT",
  "icon": "assets/customizableelytra/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "com.hidoni.customizableelytrafabric.client.CustomizableElytra"
    ],
    "main": [
      "com.hidoni.customizableelytrafabric.CustomizableElytra"
    ]
  },
  "mixins": [
    "customizableelytra.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.6",
    "fabric": ">=0.36.0",
    "minecraft": ">=1.17",
    "caelus": "*"
  }
}
